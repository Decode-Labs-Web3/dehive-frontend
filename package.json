{
  "name": "dehive-frontend",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev -p 9000",
    "dev:fast": "next dev -p 9000 --turbo",
    "build": "next build --webpack",
    "build:analyze": "ANALYZE=true next build --webpack",
    "build:profile": "next build --profile",
    "start": "next start -p 9000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "postbuild": "next-sitemap",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:9000 && NODE_ENV=development electron .\"",
    "electron-pack": "npm run build && electron-builder",
    "electron-pack-mac": "npm run build && electron-builder --mac",
    "electron-pack-win": "npm run build && electron-builder --win",
    "electron-pack-linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/utilities": "^3.2.2",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^3.0.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@reduxjs/toolkit": "^2.10.1",
    "@stream-io/node-sdk": "^0.7.12",
    "@stream-io/video-client": "^1.34.1",
    "@stream-io/video-react-sdk": "^1.24.1",
    "@tanstack/react-query": "^5.90.7",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.546.0",
    "merkletreejs": "^0.6.0",
    "next": "^16.0.1",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-redux": "^9.2.0",
    "react-webcam": "^7.2.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "valtio": "^2.2.0",
    "viem": "^2.38.6",
    "wagmi": "^2.19.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^16.0.1",
    "@next/mdx": "^16.0.1",
    "@types/connect": "^3.4.38",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/trusted-types": "^2.0.7",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "critters": "^0.0.23",
    "electron": "^39.1.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.1",
    "lightningcss": "^1.30.2",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "use-debounce": "^10.0.6",
    "wait-on": "^9.0.3",
    "web-vitals": "^5.1.0"
  },
  "build": {
    "appId": "com.dehive.frontend",
    "productName": "DeHive",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      ".next/**/*",
      "public/**/*",
      "node_modules/**/*",
      "package.json",
      "next.config.ts"
    ],
    "mac": {
      "category": "public.app-category.social-networking",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "public/images/logos/tokens/dehive-logo.png"
    },
    "win": {
      "target": "nsis",
      "icon": "public/images/logos/tokens/dehive-logo.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/images/logos/tokens/dehive-logo.png"
    },
    "afterPack": "./electron/after-pack.js"
  }
}
